<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController">
   <items>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="952.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Button fx:id="bthAddEvent" layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onAction="#addEvent" prefHeight="31.0" prefWidth="98.0" text="Add Event">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <DatePicker fx:id="datePicker" layoutX="139.0" layoutY="12.0" prefHeight="26.0" prefWidth="138.0" />
            <TextField fx:id="txtEvent" layoutX="322.0" layoutY="12.0" prefHeight="26.0" prefWidth="606.0" promptText="Add Your Event Here" />
         </children>
      </AnchorPane>
      <ListView fx:id="eventList" prefHeight="200.0" prefWidth="200.0" />
   </items>
</SplitPane>
